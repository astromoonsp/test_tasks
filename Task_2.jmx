            <JSR223Assertion guiclass="TestBeanGUI" testclass="JSR223Assertion" testname="Extract breed and country" enabled="true">
              <stringProp name="cacheKey">false</stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="script">
import groovy.json.JsonSlurper

Random random = new Random()
responseString = SampleResult.getResponseDataAsString()
def jsonSlurper = new JsonSlurper()
def responseJSON = jsonSlurper.parseText(responseString)

int randomNumber = random.nextInt(responseJSON.total)

randomCat = responseJSON.data[randomNumber]

vars.put(&quot;catBreed&quot;, randomCat.breed.toString());
vars.put(&quot;catCountry&quot;, randomCat.country.toString());
</stringProp>
